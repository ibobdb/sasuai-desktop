    "lucide-react": "^0.501.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.6.7",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^14.1.0",
    "react-top-loading-bar": "^3.0.2",
